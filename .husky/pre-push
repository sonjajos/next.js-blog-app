#!/usr/bin/env sh
. "$(dirname -- "$0")/_/husky.sh"

BRANCH=$(git symbolic-ref --short HEAD)

if [ "$BRANCH" = "main" ] || [ "$BRANCH" = "develop" ]; then
  echo "üîç Running build check on branch: $BRANCH"
  npm run build
else
  echo "‚è≠Ô∏è Skipping build check on branch: $BRANCH"
fi

npm run build
